{
  "expo": {
    "name": "shameela",
    "slug": "shameela",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdikarya.shameela"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "deep-linking",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c6d70029-4134-4e7e-98e8-c352e6eb4c3f"
      }
    }
  }
}
